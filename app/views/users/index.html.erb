 



<% if current_user_admin? %>
       <section class="admin">  
     <%= link_to "users", users_path, class: "button" %>

     </section>
     <% end %>



<h1><%= pluralize(@users.size, "User") %></h1>
<ul class="users">
  <% @users.each do |user| %>
    <li>

      <%= link_to user.name, user %> role
      <p>
      <%= link_to user.role %>
      created
      <%= time_ago_in_words(user.created_at) %> ago
      </p>
    </li>

  <% end %>
</ul>


